@article{pitera_comparison_2002,
	title = {A comparison of non-bonded scaling approaches for free energy calculations},
	volume = {28},
	abstract = {One of the long-standing problems in free energy calculations that involve the annihilation of particles is the choice of how to scale the non-bonded interactions of the disappearing particle. We make use of a ŉull transformation' (converting naphthalene to naphthalene) to extensively test and compare a number of scaling approaches, including simple linear, quadratic, mixed non-linear and separation-shifted ('soft-core') scaling. While any protocol that involves some sort of non-linear scaling yields reasonable free energy results, only the 'soft-core' scaling methods yield completely stable dynamics near the simulation end-points. Additional advantages of this method in the context of complex molecular conversions are also discussed.},
