@article{lepre_solution_1994,
	title = {{SOLUTION} {STRUCTURE} {OF} {FK506} {BOUND} {TO} {THE} {R42K} ; {H87V} {DOUBLE} {MUTANT} {OF} {FKBP-12}},
	volume = {33},
	abstract = {The binding of the {FK506/FKBP-12} complex to calcineurin {(CN),} its putative target for immunosuppression, involves recognition of solvent-exposed regions of the ligand as well as {FKBP-12} residues near the active site. The {R42K,} {H87V} double muta tion of {FKBP-12} decreases the {CN} affinity of the complex by 550-fold {[Aldape,} R. A., Futer, O., {DeCenzo,} M. T., Jarrett, B. P., Murcko, M. A., and Livingston, D. J. (1992) J. Biol. Chem. 267, 16029-16032]. This work reports the solution structure of C-13- labeled {FK506} bound to {R42K,} {H87V} {FKBP-12.} Assignments and {NOE} measurements at three mixing times were made from inverse-detected {H-1-C-13} {NMR} experiments. Structures were calculated by several different methods, including distance geometry, restrained mo lecular dynamics, and molecular dynamics with time-averaged restraints. The {NMR} structures of the ligand are very well defined by the {NOE} restraints and differ slightly from the X-ray structure in regions that are involved in crystal packing. Comparison w ith the {NMR} structure of {FK506} bound to wild-type {FKBP-12} reveals that the {R42K,} {H87V} mutation causes the ligand backbone near C16 to move by 2.5 to 4.5 Angstrom, reorients {15-MeO} by 90 degrees, and shifts {13-MeO} by approximately 1.5 Angstrom. {FK506} appea rs to undergo a concerted, mutationally induced shift in the binding pocket, with the greatest changes occurring in the effector region of the drug. The altered effector conformation of mutant-bound {FK506} may perturb interactions between the drug and {CN,} thus accounting for the effect of the double mutation upon the {CN} inhibitory activity of the complex.},
	number = {46},
	journal = {{Biochemistry-US}},
	author = {C. A {LEPRE} and D. A {PEARLMAN} and J. W {CHENG} and M. T {DECENZO} and D. J {LIVINGSTON} and J. M {MOORE}},
	month = nov,
	year = {1994},
	keywords = {;, {BINDING-PROTEIN,} {CALCINEURIN,} {COMPLEXES,} {CYCLOSPORINE-A,} {DISTANCE,} {FK506,} {IMMUNOPHILIN,} {IMMUNOSUPPRESSANT,} {LIGAND,} {MOLECULAR-DYNAMICS,} {NUCLEAR-MAGNETIC-RESONANCE,} {PHOSPHATASE-ACTIVITY,} {RAPAMYCIN,} {RESTRAINTS}},
	pages = {13571―13580}
},

