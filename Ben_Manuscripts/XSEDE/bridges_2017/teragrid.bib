@article{straatsma_holonomic_1992,
	title = {{HOLONOMIC} {CONSTRAINT} {CONTRIBUTIONS} {TO} {FREE-ENERGY} {DIFFERENCES} {FROM} {THERMODYNAMIC} {INTEGRATION} {MOLECULAR-DYNAMICS} {SIMULATIONS}},
